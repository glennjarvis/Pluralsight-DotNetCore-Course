@page "{shopId:int}"


@model WebApplication1.Pages.Shops.DetailModel
@{
    ViewData["Title"] = "Detail";
}

<h2>@Model.shop.Name</h2>

@if  (Model.Message != null)
{
    <div class="alert alert-info">@Model.Message</div>
}

<div>ID: @Model.shop.Id</div>

<div>Location: @Model.shop.Location</div>

<div>Specialty: @Model.shop.Specialty</div>

<a asp-page="./List" class="btn btn-primary">All Shops</a>



